{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:backend": "npm run build:backend:cross-platform",
    "build:backend:unix": "sh ../backend/build_backend.sh",
    "build:backend:windows": "powershell -ExecutionPolicy Bypass -File ../backend/build_backend.ps1",
    "build:backend:windows-batch": "../backend/build_backend.bat",
    "build:backend:cross-platform": "node -e \"const os = require('os'); const { execSync } = require('child_process'); const isWindows = os.platform() === 'win32'; const command = isWindows ? 'npm run build:backend:windows' : 'npm run build:backend:unix'; execSync(command, { stdio: 'inherit' });\"",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
