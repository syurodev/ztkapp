<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.zkteco.service</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/ZKTeco.app/Contents/Resources/venv/bin/python</string>
        <string>/Applications/ZKTeco.app/Contents/Resources/service_app.py</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Applications/ZKTeco.app/Contents/Resources</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/zkteco-service.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/zkteco-service-error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PYTHONPATH</key>
        <string>/Applications/ZKTeco.app/Contents/Resources</string>
    </dict>
</dict>
</plist>